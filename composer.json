{
	"require": {
		"phpdocumentor/phpdocumentor": "dev-master",
		"phpunit/dbunit": "@stable",
		"phpunit/phpunit": "@stable",
		"pusher/pusher-php-server": "@stable",
		"ramsey/uuid": "@stable"
	},

	"scripts": {
		"phpdoc": "./vendor/bin/phpdoc run -d ./php/classes -t doc",
		"phpunit": "./vendor/bin/phpunit -c ./php/classes/Test --coverage-html coverage"
	}
}